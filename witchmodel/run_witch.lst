30656                  MCOST_INV('edv',t,n) =e= (glider_manufacture_cost + size_battery('edv',n) * MCOST_INV('battery',t,n) + ELMOTOR_COST(t)*size_elmotor('edv') + charger_cost + charging_station)/(1e6*reg_discount_veh(n));
