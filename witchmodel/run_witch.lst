16931                  MCOST_INV('edv_stfr',t,n) =e= (inv_cost_frt('trad_stfr') *0.95 + size_battery_freight('edv_stfr',n) * MCOST_INV('battery',t,n) * 1.7 + disutility_costs_stfr('edv_stfr',t,n)) * 1e-6;
