28712                  MCOST_INV('plg_hbd_stfr',t,n) =e= (inv_cost_frt('trad_stfr') + (size_battery_freight('plg_hbd_stfr',n) * MCOST_INV('battery',t,n) * 2.23 ) + disutility_costs_stfr('plg_hbd_stfr',t,n)) * 1e-6;
