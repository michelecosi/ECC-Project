28115                  MCOST_INV(jrd,t,n) =e= MCOST_INV(jrd,tfirst,n) *
28116                                         (sum(rd$(sameas(rd,jrd)), K_RD(rd,tt,n)/wkrd0(rd)))**rd_coef(jrd,'lbr') *
28117                                         (wcum(jrd,t)/wcum(jrd,tfirst))**rd_coef(jrd,'lbd');
