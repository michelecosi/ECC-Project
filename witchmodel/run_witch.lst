46853  k_en_windoff_norm(wind_dist,wind_depth,wind_class,t,n) = K_EN_WINDOFF.l(wind_dist,wind_depth,wind_class,t,n) / K_EN_WINDOFF.up(wind_dist,wind_depth,wind_class,t,n);
