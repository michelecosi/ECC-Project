12340                                         (sum(rd$(sameas(rd,jrd)), K_RD(rd,tt,n)/wkrd0(rd)))**rd_coef(jrd,'lbr') *
12341                                         (wcum(jrd,t)/wcum(jrd,tfirst))**rd_coef(jrd,'lbd');
