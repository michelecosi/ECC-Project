27959                      ((sum(n$map_clt_n('c_oceania',n),
27960                        (l(t,n) / sum(nn$map_clt_n('c_oceania',nn), l(t,nn))) *
27961                        (Q.l('CC',t,n) / l(t,n))**(1 - gamma))
