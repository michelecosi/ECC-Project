37715                      PROB(t) * stpf(t) / sum(tt$(year(tt) le yeoh), PROB(tt) * stpf(tt)) *
37716                      ((sum(n$map_clt_n('c_te',n),
37717                        (l(t,n) / sum(nn$map_clt_n('c_te',nn), l(t,nn))) *
37718                        (Q.l('CC',t,n) / l(t,n))**(1 - gamma))
37719                       )**(1 / (1 - gamma))
37720                      )**(1 - eta) )
