45865                              ( tlen(t) * wemi('co2',t) )$( sameas(m,'atm') );
45866   
45867  WCUM_EMI.l(oghg,'atm',tp1,n)$(not tfix(tp1)) = WCUM_EMI.l(oghg,'atm',t,n)*cmdec1(oghg)**tlen(t)
45868                                   +cmdec2(oghg)*(wemi(oghg,t) +wemi(oghg,tp1))/2
