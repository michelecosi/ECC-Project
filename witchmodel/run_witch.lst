18848                  MCOST_INV('hbd_stfr',t,n) =e= (inv_cost_frt('trad_stfr') + (size_battery_freight('hbd_stfr',n) * MCOST_INV('battery',t,n) * 1.2) + disutility_costs_stfr('hbd_stfr',t,n)) / (reg_discount_veh(n) * 1e6);
