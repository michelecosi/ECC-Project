12334                  K_RD(rd,tp1,n) =e= K_RD(rd,t,n)*(1-rd_delta(rd))**tlen(t)
12335                                        +tlen(t)*rd_coef(rd,'a')*I_RD(rd,t,n)**rd_coef(rd,'b') *
12336                                        K_RD(rd,t,n)**rd_coef(rd,'c') *
12337                                        SPILL(rd,t,n)**rd_coef(rd,'d');
